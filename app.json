{
  "expo": {
    "name": "VNH",
    "slug": "emu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/marca.png",
    "scheme": "VNH",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/marca.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "br.ars.vnh"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/marca.png",
      "backgroundColor": "#000000",
      "themeColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/marca.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "03fe0ce4-c3c1-4c14-88f9-dd4002d1ce70"
      }
    },
    "owner": "newfacek"
  }
}
